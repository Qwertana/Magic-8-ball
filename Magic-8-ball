import random
quest = input("Question:  ")
num = random.randint(1, 9)

if num == 1:
 awswer = "Yes definitely"
elif num == 2:
 awswer = "It is decidedly so"
elif num == 3:
 awswer = "Without a doubt"
elif num == 4:
 awswer = "Reply hazy, try again"
elif num == 5:
 awswer = "Ask again later"
elif num == 6:
 awswer = "Better not tell you now"
elif 7:
 awswer = "My sources say no"
elif num == 8:
 awswer = "Outlook not so good"  
elif num == 9:
 awswer = "Very doubtful"  
else:
 awswer = "ERROR"

print("Magic 8 ball:  " + awswer)
